<h1>All Users</h1>
<p> Each Id link to the show page of user details </p>
<ul>
  <li><%= link_to "Create", new_user_path %></li>
</ul>
<table>
  <tr>
      <th> Id </th>
      <th> First Name </th>
      <th> Last Name </th>
      <th> Email </th>
  </tr>
  <tr>
  <% @users.each do |user| %>
    <tr>
      <td> <%= link_to user.id, user %> </td>
      <td> <%= user.first_name %> </td>
      <td> <%= user.last_name %> </td>
      <td> <%= user.email %> </td>
    </tr>
      <% end %>
  </tr>

</table>
